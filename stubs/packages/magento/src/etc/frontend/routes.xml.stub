<?xml version="1.0"?>
<!--
/**
 * Frontend Routes Configuration
 *
 * This file defines the frontend route for the module.
 * The module will be accessible at: /{{PACKAGE_NAME_NORMALIZED}}/*
 */
-->
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../vendor/magento/framework/App/etc/routes.xsd">
    <router id="standard">
        <route id="{{ROUTE_ID}}" frontName="{{PACKAGE_NAME_NORMALIZED}}">
            <module name="{{MODULE_NAME}}" />
        </route>
    </router>
</config>
